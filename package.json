{
  "name": "rule-of-thirds-web-app",
  "version": "1.0.0",
  "description": "A web application for Rule of Thirds signal orchestration and product intelligence",
  "type": "module",
  "main": "build/httpServer.js",
  "scripts": {
    "build": "tsc && npm --prefix web run build",
    "start": "node build/httpServer.js",
    "dev": "npm run build && node build/httpServer.js",
    "dev:backend": "tsc && node build/httpServer.js",
    "dev:frontend": "npm --prefix web run dev",
    "dev:full": "concurrently \"npm run dev:backend\" \"wait-on http://localhost:3001/api/health && npm run dev:frontend\""
  },
  "keywords": [
    "rule-of-thirds",
    "signal-orchestration",
    "product-intelligence",
    "external-signals",
    "internal-research",
    "product-metrics",
    "web-app"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
